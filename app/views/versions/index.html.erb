<h1><%= @application.name %>'s Versions</h1>
<table cellpadding="0" cellspacing="0" border="0" class="display" id="versions" width="100%" data-source="<%= application_versions_url(format: "json") %>" >
  <thead>
  <tr>
    <th>Sr. No.</th>
    <th>Version</th>
    <th>Created Date & Time</th>
    <th>Updated Date & Time</th>
    <th>Actions</th>
  </tr>
  </thead>
<tbody>

</tbody>
</table>

<br/>
<%= button_to 'New Version', new_application_version_path(@application), :method => :get %><br/>
<%= button_to 'Applications', applications_path, :method => :get %><br/>
<%= button_to 'Refresh', href="javascript:location.reload(true)" %>
